<div id="main_gallery_wrapper">
    <div id="gallery_contents_wrapper">
        <div class="wrapper">
            <div id="contents">
                <%= render 'run/fullscreen/fullscreen' %>
                <%= render 'base/csscode' %>
                <%= render 'base/values', pagevalues: @pagevalues, is_runwindow_reload: @is_runwindow_reload %>
            </div>
            <%= render 'gallery/sidebar/popup' %>
        </div>
    </div>
</div>

<%= javascript_include_tag 'pipeline/gallery_with_run', 'data-turbolinks-track' => true %>
<%= javascript_include_tag 'pipeline/preload_item', 'data-turbolinks-track' => true %>