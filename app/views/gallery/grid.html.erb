<form name="send_form">
    <div id="grid_centering_wrapper">
        <div id="grid_wrapper">
            <% @contents.each do |content| %>
                <%= render 'gallery/grid_contents', content: content %>
            <% end %>
        </div>
    </div>
</form>

<%= stylesheet_link_tag 'pipeline/gallery', media: 'all', 'data-turbolinks-track' => true %>
<%= javascript_include_tag 'pipeline/gallery', 'data-turbolinks-track' => true %>