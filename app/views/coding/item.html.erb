<div id="contents">
    <div id="wrapper">
        <div id="tree_wrapper">
            <div id="tree">
                <ul>
                    <li class='dir'><%= @user.name %></li>
                    <%== @load_tree_data %>
                </ul>
            </div>
        </div>
        <div id="editor_wrapper">
            <% if @load_editor_data.length > 0 %>
                <ul id="my_tab" class="nav nav-tabs">
                    <% @load_editor_data.each do |data| %>
                        <li class="<%= data['is_front']? 'active' : '' %>"><a href="#<%= "uc_#{data['id'].to_s}_wrapper" %>" data-toggle="tab"><%= data['name'] %></a></li>
                    <% end %>
                </ul>
                <div id="my_tab_content" class="tab-content">
                    <% @load_editor_data.each do |data| %>
                        <div class="tab-pane fade in <%= data['is_front']? 'active' : '' %>" id="<%= "uc_#{data['id'].to_s}_wrapper" %>" >
                            <div id="<%= "uc_#{data.id.to_s}" %>" class="editor <%= data['lang_type'] %>"></div>
                        </div>
                    <% end %>
                </div>
            <% end %>
        </div>
    </div>
</div>